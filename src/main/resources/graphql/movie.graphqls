scalar Instant

type Movie {
    id: ID!
    name: String!
    category: Category!
    tags: [String]
    score: Int
    createdBy: String
    createdAt: Instant
    modifiedBy: String
    modifiedAt: Instant
}

input MovieInput {
    name: String!
    category: Category!
    score: Int!
    tags: [String]
}

input MovieCriteria {
    name: String
    category: Category
    tag: String
}